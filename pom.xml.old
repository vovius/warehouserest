<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.dio</groupId>
  <artifactId>warehouserest</artifactId>
  <packaging>war</packaging>
  <version>0.0.1-SNAPSHOT</version>
  <name>warehouserest Maven Webapp</name>
  <url>http://maven.apache.org</url>

  <build>
    <finalName>warehouserest</finalName>
  </build>
  
  <properties>
    <jersey.server>2.18</jersey.server>
    <slf4jVersion>1.6.1</slf4jVersion>
    <spring.version>4.1.6.RELEASE</spring.version>
  </properties>
  
  <dependencies>
  
    <dependency>
      <groupId>com.dio.javamentoring</groupId>
      <artifactId>warehouse</artifactId>
      <version>0.0.1-SNAPSHOT</version>      
    </dependency>
  
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    
		<dependency>
		  <groupId>org.glassfish.jersey.core</groupId>
		  <artifactId>jersey-server</artifactId>
		  <version>${jersey.server}</version>
